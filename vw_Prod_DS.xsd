                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        Historia.Serie, Historia.Numero, Historia.Fecha, RTRIM(Historia.Serie) + CONVERT(varchar, Historia.Numero) AS Factura, Historia.Anexo, Historia.Letra, CONT_FechasAplicacion.Estatus, Historia.id_historia
FROM            Historia INNER JOIN
                         CONT_FechasAplicacion ON Historia.Fecha = CONVERT(varchar, CONT_FechasAplicacion.Fecha, 112)
WHERE        (NOT ((RTRIM(Historia.Serie) + CONVERT(varchar, Historia.Numero)) IN
                             (SELECT        Hisgin_1.Factura
                               FROM            Hisgin AS Hisgin_1 INNER JOIN
                                                         CONT_FechasAplicacion AS CONT_FechasAplicacion_1 ON Hisgin_1.Fepag = CONT_FechasAplicacion_1.Fecha
                               WHERE        (CONT_FechasAplicacion_1.Estatus = N'Vigente')))) AND (CONT_FechasAplicacion.Estatus = N'Vigente') AND (Historia.Serie &lt;&gt; N'B')</CommandText>
